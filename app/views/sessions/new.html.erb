<h1>ログインはこちら</h1>
<%= form_with(scope: :session, url: sessions_path, local: true) do |f| %>
  <%= f.label :email %>
  <%= f.email_field :email %><br>
  <%= f.label :password %>
  <%= f.password_field :password %><br>
  <%= f.submit "Log in" %>
<% end %><br>
<%= link_to "新規アカウント登録はこちら", new_user_path %><br>
<%= link_to "みんなの投稿はこちら", pictures_path %>